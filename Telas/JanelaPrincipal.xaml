<Window x:Class="IBeauty.JanelaPrincipal"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:controle="clr-namespace:IBeauty.Controle"
        Title="IBeauty"  Height="1080" Width="1920"
        WindowState="Maximized"
        Loaded="JanelaPrincipal_Loaded">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>

        <Frame x:Name="MainFrame" Margin="0,40,0,0" NavigationUIVisibility="Hidden" Panel.ZIndex="-1" Grid.Row="1"/>

        <controle:MenuLogin x:Name="menuLogin" Visibility="Visible" Grid.Row="1"/>
        <controle:MenuControl x:Name="menuControl" Visibility="Collapsed" Grid.Row="1"/>
    </Grid>
</Window>
